{
  "$schema": "https://typedoc.org/schema.json",
  "name": "Aleph Indexer Framework for Solana/Ethereum/BSC",
  "entryPointStrategy": "packages",
  "entryPoints": [
    "packages/core",
    "packages/framework",
    "packages/solana",
    "packages/ethereum",
    "packages/bsc"
  ],
  "out": "docs",
  "externalPattern": [
    "**/lib/**",
    "**/external/**"
  ],
  "excludeExternals": true
}